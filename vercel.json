{
    "version": 2,
    "builds": [
        {
            "src": "/api/hello_world.py",
            "use": "@vercel/python"
        },
        {
            "src": "/api/another_endpoint.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/hello_world.py"
        },
        {
            "src": "/(.*)",
            "dest": "/api/another_endpoint.py"
        }
    ]
}